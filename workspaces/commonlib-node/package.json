{
  "name": "commonlib-node",
  "version": "0.1.0",
  "private": false,
  "description": "Common functions for node-based server use",
  "keywords": [],
  "author": "Adam Saunders <saunders@cs.umass.edu>",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": [
    "dist/index.d.ts"
  ],
  "directories": {
    "src": "src"
  },
  "scripts": {
    "compile": "tsc",
    "build": "rollup -c",
    "lint": "eslint --config .eslintrc.js --ext .ts --ignore-path ../../.gitignore ./src",
    "lint:fix": "eslint --config .eslintrc.js --ext .ts --ignore-path ../../.gitignore ./src --fix",
    "test": "jest"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "commonlib-shared": "0.1.0",
    "fast-csv": "^4.3.5",
    "fp-ts": "2.9.1",
    "fs-extra": "^9.0.1",
    "io-ts": "^2.2.13",
    "pumpify": "^2.0.1",
    "split": "^1.0.1",
    "through2": "^4.0.2",
    "typescript": "^4.1.2",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^9.0.4",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
    "@types/split": "^1.0.0",
    "@types/through2": "^2.0.36",
    "@types/yargs": "^15.0.11",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "rollup": "^2.34.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/src/$1"
    }
  }
}
