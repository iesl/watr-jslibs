{
  "name": "commonlib-shared",
  "version": "0.1.0",
  "private": false,
  "description": "Common functions for webclient/server use",
  "keywords": [],
  "author": "Adam Saunders <saunders@cs.umass.edu>",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": [
    "dist/index.d.ts"
  ],
  "directories": {
    "src": "src"
  },
  "scripts": {
    "build": "tsc",
    "rollup": "rollup -c",
    "lint": "eslint --config .eslintrc.js --ext .ts --ignore-path ../../.gitignore ./src",
    "lint:fix": "eslint --config .eslintrc.js --ext .ts --ignore-path ../../.gitignore ./src --fix",
    "test": "jest"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "logform": "^2.2.0",
    "slice-ansi": "^4.0.0",
    "typescript": "^4.1.2",
    "url-parse": "^1.4.7",
    "winston": "^3.3.3",
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/chai": "^4.2.14",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/url-parse": "^1.4.3",
    "@types/wrap-ansi": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "rollup": "^2.34.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/src/$1"
    }
  }
}
