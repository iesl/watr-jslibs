{
  "name": "watrfront",
  "version": "0.1.0",
  "description": "Web Frontends for WatrWorks projects",
  "author": "Adam Saunders",
  "private": true,
  "scripts": {
    "nuxt": "nuxt",
    "build": "nuxt build",
    "build:stories": "../codemorphs/bin/gen-stories",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --no-eslintrc --config .eslintrc.js --ext .ts --ignore-path ../../.gitignore src/",
    "lint:fix": "eslint --no-eslintrc --config .eslintrc.js --ext .ts --ignore-path ../../.gitignore src/ --fix",
    "test": "jest",
    "serve:json": "json-server --port 3100 ./dev-data/db.json --static ./dev-data --routes ./dev-data/routes.json",
    "ui:dev": "nuxt",
    "dev": "npm-run-all -p -r nuxt serve:json"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/dotenv": "^1.4.1",
    "@vue/composition-api": "^1.0.0-beta.20",
    "animejs": "^3.2.1",
    "chroma-js": "^2.1.0",
    "d3-selection": "^2.0.0",
    "d3-transition": "^2.0.0",
    "fp-ts": "^2.9.0",
    "io-ts": "^2.2.13",
    "io-ts-types": "^0.5.12",
    "lodash": "^4.17.20",
    "nuxt": "^2.14.7",
    "rbush": "^3.0.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.1",
    "@nuxtjs/vuetify": "^1.11.2",
    "@types/animejs": "^3.1.2",
    "@types/chai": "^4.2.14",
    "@types/chroma-js": "^2.1.2",
    "@types/d3-selection": "^2.0.0",
    "@types/d3-transition": "^2.0.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/rbush": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "@vue/test-utils": "^1.1.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-nuxt": ">=2.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "json-server": "^0.16.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "vue-jest": "^4.0.0-0"
  }
}
